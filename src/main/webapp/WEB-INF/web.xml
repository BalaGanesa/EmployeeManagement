<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <welcome-file-list>
    <welcome-file>login.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>JDBC</servlet-name>
    <servlet-class>Logic.dbconnection</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>JDBC</servlet-name>
    <url-pattern>/JDBC</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>Logic.login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LeaveApporve</display-name>
    <servlet-name>LeaveApporve</servlet-name>
    <servlet-class>Logic.Apporve</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LeaveApporve</servlet-name>
    <url-pattern>/LeaveApporve</url-pattern>
  </servlet-mapping>
</web-app>